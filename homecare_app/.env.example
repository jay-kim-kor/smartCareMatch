# Homecare App Environment Configuration
# Copy this file to .env and update the values for your local environment

# API Base URL - Required
# Example: http://localhost:8000 or https://api.homecare.com
FLUTTER_HOMECARE_APP_API_BASE_URL=http://localhost:8000

# Development Settings
# Set to true for development mode
DEBUG_MODE=true

# API Timeout (in seconds)
API_TIMEOUT_SECONDS=10